[alias]
    br  = branch
    ci  = commit -v
    cie = commit --allow-empty
    co  = checkout
    d   = diff
    st  = status -s
[color]
    ui = auto
    status = auto
    diff = auto
    branch = auto
    interactive = auto
    grep = auto
[core]
    editor = vim
    excludesfile = ~/.gitignore
[push]
    default = simple
[user]
    name = Ryoji Yoshioka
    email = yoshioka.ryoji@gmail.com
