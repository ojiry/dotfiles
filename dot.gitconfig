[alias]
    ad = add
    br = branch
    ci = commit -v
    co = checkout
    df = diff
    st = status
[color]
    ui = auto
    status = auto
    diff = auto
    branch = auto
    interactive = auto
    grep = auto
[core]
    editor = vim
[ghq]
    root = ~/work/src
[github]
    user = ojiry
[init]
    templatedir = ~/.config/git/templates/hook
[push]
    default = simple
[user]
    name = Ryoji Yoshioka
    email = yoshioka.ryoji@gmail.com
